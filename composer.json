  "description": "Olympus Hermes is a bundle used to translate all messages from your WordPress packages and Olympus bundles.",
  "keywords": ["olympus-hermes", "olympus", "hermes", "translator", "translate", "library", "php", "wordpress"],
  "homepage": "https://github.com/GetOlympus/Hermes-Translator",
  "license": "MIT",
  "authors": [
    {
      "name": "Achraf Chouk",
      "homepage": "https://github.com/crewstyle"
    }
  ],
  "autoload": {
    "psr-4": {
      "GetOlympus\\Hermes\\": "src/Hermes/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "minimum-stability": "stable",
  "require-dev": {
    "phpunit/phpunit": "~5.3"
  }
